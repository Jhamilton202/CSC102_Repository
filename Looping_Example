    <script type="text/javascript">
        /*
            Basic control example - John Hamiton 
        */
        //Declarations 
        var sumOfnumbers = 0;
        var numberOfLoops = 10;
        //var counter = 1;
   //do {some statement} while (condition is not met)
        do {
            if(sumOfnumbers!=NaN)
               {

            sumOfnumbers += 1;//
           // counter++;  +; 
            alert(sumOfnumbers)
                }
           }
        while (sumOfnumbers <=numberOfLoops - 1);//counter <=numberOfLoops
       
    </script>
